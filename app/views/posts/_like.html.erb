<%= form_for(p.likes.build) do |f| %>
<%= hidden_field_tag :post_id, p.id %>
<% unless current_user().liked_posts.include?(p) %>
<%= f.submit "Favourite", class: ' btn btn-link', title: "Mark as Favourite" %>
<% else %>
<%= link_to "Favourated", '#', class: "btn btn-disabled" %>
<% end %>

<% end %>
